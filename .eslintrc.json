{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": 0,
    "object-curly-spacing": 0,
    "linebreak-style": 0,
    "react/prop-types": "off",
    "no-unused-vars": 0,
    "no-undef": 0
  }
}
